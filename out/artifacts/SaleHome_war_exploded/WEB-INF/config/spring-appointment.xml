       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
	<bean id="appointmentDAO" class="com.ht.dao.impl.AppointmentDAOimpl" autowire="byName"></bean>
	<bean id="appointmentService" class="com.ht.service.impl.AppointmentServiceimpl" autowire="byName"></bean>
	<bean id="appointmentAction" class="com.ht.controller.AppointmentAction" autowire="byName" scope="prototype"></bean>
