	<bean id="roomDAO" class="com.ht.dao.impl.RoomDAOimpl" autowire="byName"></bean>
	<bean id="roomService" class="com.ht.service.impl.RoomServiceimpl" autowire="byName"></bean>
	<bean id="roomAction" class="com.ht.controller.RoomAction" autowire="byName" scope="prototype"></bean>
